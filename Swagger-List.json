{
  "swagger": "2.0",
  "info": {
    "title": "SharePoint 2019 â€“ Get All List Items",
    "version": "1.0.0"
  },
  "host": "demo.traveport-test.de",
  "schemes": ["https"],
  "produces": ["application/json"],
  "paths": {
    "/{SiteUrl}/_api/web/lists/GetByTitle('{ListTitle}')/items": {
      "get": {
        "tags": ["SharePoint"],
        "summary": "Get all list items",
        "operationId": "GetAllListItems",
        "parameters": [
          {
            "name": "SiteUrl",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Site relative URL (e.g. sites/CRM)"
          },
          {
            "name": "ListTitle",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "List title"
          }
        ],
        "responses": {
          "200": {
            "description": "OData verbose list response",
            "schema": {
              "$ref": "#/definitions/ODataVerboseListResponse"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "ListItem": {
      "type": "object",
      "properties": {
        "Id": {
          "type": "integer",
          "format": "int32"
        },
        "Title": {
          "type": "string"
        }
      }
    },
    "ODataVerboseListResponse": {
      "type": "object",
      "properties": {
        "d": {
          "type": "object",
          "properties": {
            "results": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ListItem"
              }
            }
          }
        }
      }
    }
  }
