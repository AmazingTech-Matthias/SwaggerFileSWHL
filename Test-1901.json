{
  "swagger": "2.0",
  "info": {
    "title": "SharePoint 2019 List API (Id, Title)",
    "version": "1.0"
  },
  "schemes": ["http", "https"],
  "host": "demo.traveport-test.de",
  "basePath": "/raeume/projekt-54/_api",
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/web/lists/getbytitle('{listName}')/items?$select=Id,Title": {
      "get": {
        "summary": "Get list items (Id, Title only)",
        "parameters": [
          {
            "name": "listName",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/ListItemCollection"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "ListItemCollection": {
      "type": "object",
      "properties": {
        "value": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ListItem"
          }
        }
      }
    },
    "ListItem": {
      "type": "object",
      "properties": {
        "Id": {
          "type": "integer"
        },
        "Title": {
          "type": "string"
        }
      }
    }
  }
}