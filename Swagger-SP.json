{
  "swagger": "2.0",
  "info": {
    "title": "K2 SharePoint Get Items",
    "version": "1.0"
  },
  "host": "demo.traveport-test.de",
  "schemes": [
    "https"
  ],
  "paths": {
    "/{sitePath}/_api/web/lists/GetByTitle('{listTitle}')/items": {
      "get": {
        "operationId": "GetItems",
        "parameters": [
          {
            "name": "sitePath",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "listTitle",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    }
  }
}

